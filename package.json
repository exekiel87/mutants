{
  "name": "mutants",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test:server": "node spec/specs.js",
    "test:server:coverage": "nyc node spec/specs.js",
    "setup": "node scripts/migrate --clean --up"
  },
  "nyc": {
    "report-dir": "./spec/istanbul/report",
    "temp-dir": "./spec/istanbul",
    "reporter": [
      "text",
      "text-summary",
      "html"
    ],
    "exclude": [
      "spec/**/*",
      "server/*.spec.js"
    ]
  },
  "dependencies": {
    "@hapi/boom": "^9.1.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jasmine": "^3.6.3",
    "joi": "^17.3.0",
    "mongodb": "^3.6.3",
    "morgan": "~1.9.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-core": "^3.6.0",
    "nodemon": "^2.0.6",
    "watchify": "^3.11.1"
  }
}
